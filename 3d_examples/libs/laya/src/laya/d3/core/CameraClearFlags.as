
/**
 * 相机清除标记。
 */
package laya.d3.core {

	public class CameraClearFlags {

		/**
		 * 固定颜色。
		 */
		public static var SolidColor:Number = 0;

		/**
		 * 天空。
		 */
		public static var Sky:Number = 1;

		/**
		 * 仅深度。
		 */
		public static var DepthOnly:Number = 2;

		/**
		 * 不清除。
		 */
		public static var Nothing:Number = 3;

	}
}